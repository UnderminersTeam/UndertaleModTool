<local:DataUserControl x:Class="UndertaleModTool.UndertaleParticleSystemEmitterEditor"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                       xmlns:local="clr-namespace:UndertaleModTool"
                       xmlns:undertale="clr-namespace:UndertaleModLib.Models;assembly=UndertaleModLib"
                       xmlns:sys="clr-namespace:System;assembly=mscorlib"
                       mc:Ignorable="d" 
                       d:DesignHeight="4500" d:DesignWidth="800" d:DataContext="{d:DesignInstance undertale:UndertaleParticleSystemEmitter}" xmlns:strings="clr-namespace:UndertaleModTool.Resources" >
    <UserControl.Resources>
        <ResourceDictionary>
            <local:IsVersionAtLeastConverter x:Key="IsVersionAtLeastConverter"/>
            <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="EmitMode">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="undertale:UndertaleParticleSystemEmitter+EmitMode" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="DistributionEnum">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="undertale:UndertaleParticleSystemEmitter+DistributionEnum" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="EmitterShape">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="undertale:UndertaleParticleSystemEmitter+EmitterShape" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="TextureEnum">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="undertale:UndertaleParticleSystemEmitter+TextureEnum" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ObjectDataProvider MethodName="GetValues" ObjectType="{x:Type sys:Enum}" x:Key="TimeUnitEnum">
                <ObjectDataProvider.MethodParameters>
                    <x:Type TypeName="undertale:UndertaleParticleSystemEmitter+TimeUnitEnum" />
                </ObjectDataProvider.MethodParameters>
            </ObjectDataProvider>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Controls/TransparencyGridBrush.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Grid.Column="0" Margin="3" Text="">Name</TextBlock>
        <local:UndertaleStringReference Grid.Row="0" Grid.Column="1" Margin="3" ObjectReference="{Binding Name}"/>

        <TextBlock Grid.Row="1" Grid.Column="0" Margin="3"
                   Visibility="{Binding Mode=OneTime, Converter={StaticResource IsVersionAtLeastConverter},ConverterParameter=2023.6}"
                   Text="{x:Static strings:Strings.Enabled}"></TextBlock>
        <CheckBox Grid.Row="1" Grid.Column="1" Margin="3" IsChecked="{Binding Enabled}"
                   Visibility="{Binding Mode=OneTime, Converter={StaticResource IsVersionAtLeastConverter},ConverterParameter=2023.6}"/>

        <TextBlock Grid.Row="2" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Emit_mode}"></TextBlock>
        <local:ComboBoxDark Grid.Row="2" Grid.Column="1" Margin="3"
                            ItemsSource="{Binding Source={StaticResource EmitMode}}" SelectedItem="{Binding Mode}"/>

        <TextBlock Grid.Row="3" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Emit_count}"></TextBlock>
        <local:TextBoxDark Grid.Row="3" Grid.Column="1" Margin="3" Text="{Binding EmitCount}"/>

        <Grid Grid.Row="4" Grid.ColumnSpan="2" Visibility="{Binding Mode=OneTime, Converter={StaticResource IsVersionAtLeastConverter},ConverterParameter=2023.8}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="3*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Emit_relative}"></TextBlock>
            <CheckBox Grid.Row="0" Grid.Column="1" Margin="3" IsChecked="{Binding EmitRelative}"/>

            <TextBlock Grid.Row="1" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Delay_min_max}"></TextBlock>
            <Grid Grid.Row="1" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding DelayMin}"/>
                <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding DelayMax}"/>
            </Grid>

            <TextBlock Grid.Row="2" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Delay_measured_in}"></TextBlock>
            <local:ComboBoxDark Grid.Row="2" Grid.Column="1" Margin="3"
                    ItemsSource="{Binding Source={StaticResource TimeUnitEnum}}" SelectedItem="{Binding DelayUnit}"/>

            <TextBlock Grid.Row="3" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Interval_min_max}"></TextBlock>
            <Grid Grid.Row="3" Grid.Column="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding IntervalMin}"/>
                <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding IntervalMax}"/>
            </Grid>

            <TextBlock Grid.Row="4" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Interval_measured_in}"></TextBlock>
            <local:ComboBoxDark Grid.Row="4" Grid.Column="1" Margin="3"
                    ItemsSource="{Binding Source={StaticResource TimeUnitEnum}}" SelectedItem="{Binding IntervalUnit}"/>
        </Grid>

        <TextBlock Grid.Row="5" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Distribution}"></TextBlock>
        <local:ComboBoxDark Grid.Row="5" Grid.Column="1" Margin="3"
                            ItemsSource="{Binding Source={StaticResource DistributionEnum}}" SelectedItem="{Binding Distribution}"/>

        <TextBlock Grid.Row="6" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Shape}"></TextBlock>
        <local:ComboBoxDark Grid.Row="6" Grid.Column="1" Margin="3"
                            ItemsSource="{Binding Source={StaticResource EmitterShape}}" SelectedItem="{Binding Shape}"/>

        <TextBlock Grid.Row="7" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Region_X_Y_Width_Height}"></TextBlock>
        <Grid Grid.Row="7" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding RegionX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding RegionY}"/>
            <local:TextBoxDark Grid.Column="2" Margin="3" Text="{Binding RegionWidth}"/>
            <local:TextBoxDark Grid.Column="3" Margin="3" Text="{Binding RegionHeight}"/>
        </Grid>

        <TextBlock Grid.Row="8" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Rotation}"></TextBlock>
        <local:TextBoxDark Grid.Row="8" Grid.Column="1" Margin="3" Text="{Binding Rotation}"/>

        <TextBlock Grid.Row="9" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Sprite}"></TextBlock>
        <local:UndertaleObjectReference Grid.Row="9" Grid.Column="1" Margin="3" ObjectReference="{Binding Sprite}" ObjectType="{x:Type undertale:UndertaleSprite}"/>

        <ListView Grid.Row="10" Grid.Column="1" ItemsSource="{Binding Sprite.Textures, Mode=OneWay}" Background="Transparent" BorderThickness="0">
            <ListView.ItemsPanel>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"/>
                </ItemsPanelTemplate>
            </ListView.ItemsPanel>
            <ListView.ItemTemplate>
                <DataTemplate>
                    <Viewbox Stretch="Uniform" StretchDirection="DownOnly">
                        <Border Background="{DynamicResource TransparencyGridBrushBrush}">
                            <local:UndertaleTexturePageItemDisplay DataContext="{Binding Texture, Mode=OneWay}"/>
                        </Border>
                    </Viewbox>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>

        <TextBlock Grid.Row="11" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Sprite_frame_index}"></TextBlock>
        <local:TextBoxDark Grid.Row="11" Grid.Column="1" Margin="3" Text="{Binding FrameIndex}"/>

        <Grid Grid.Row="12" Grid.ColumnSpan="2" Visibility="{Binding Mode=OneTime, Converter={StaticResource IsVersionAtLeastConverter},ConverterParameter=2023.4}">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
                <ColumnDefinition Width="3*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>

            <TextBlock Grid.Row="0" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Animate_sprite}"></TextBlock>
            <CheckBox Grid.Row="0" Grid.Column="1" Margin="3" IsChecked="{Binding Animate}"/>

            <TextBlock Grid.Row="1" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Stretch_sprite}"></TextBlock>
            <CheckBox Grid.Row="1" Grid.Column="1" Margin="3" IsChecked="{Binding Stretch}"/>

            <TextBlock Grid.Row="2" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Random}"></TextBlock>
            <CheckBox Grid.Row="2" Grid.Column="1" Margin="3" IsChecked="{Binding IsRandom}"/>
        </Grid>

        <TextBlock Grid.Row="13" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Texture}"></TextBlock>
        <local:ComboBoxDark Grid.Row="13" Grid.Column="1" Margin="3"
                            ItemsSource="{Binding Source={StaticResource TextureEnum}}" SelectedItem="{Binding Texture}"/>

        <TextBlock Grid.Row="14" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Start_color}"></TextBlock>
        <local:ColorPicker Grid.Row="14" Grid.Column="2" Margin="3" Color="{Binding StartColor}"/>

        <TextBlock Grid.Row="15" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Middle_color}"></TextBlock>
        <local:ColorPicker Grid.Row="15" Grid.Column="2" Margin="3" Color="{Binding MidColor}"/>

        <TextBlock Grid.Row="16" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.End_color}"></TextBlock>
        <local:ColorPicker Grid.Row="16" Grid.Column="2" Margin="3" Color="{Binding EndColor}"/>

        <TextBlock Grid.Row="17" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Additive_color_blend}"></TextBlock>
        <CheckBox Grid.Row="17" Grid.Column="1" Margin="3" IsChecked="{Binding AdditiveBlend}"/>

        <TextBlock Grid.Row="18" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Lifetime_min_max}"></TextBlock>
        <Grid Grid.Row="18" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding LifetimeMin}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding LifetimeMax}"/>
        </Grid>

        <TextBlock Grid.Row="19" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Scale_X_Y}"></TextBlock>
        <Grid Grid.Row="19" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding ScaleX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding ScaleY}"/>
        </Grid>

        <TextBlock Grid.Row="20" Grid.Column="1" Margin="3" HorizontalAlignment="Center" Foreground="DarkGray" FontStyle="Italic" TextWrapping="Wrap" Text="{x:Static strings:Strings.Hint__Prior_to_GM_2023_8__these}">
            
        </TextBlock>

        <TextBlock Grid.Row="21" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Size_min}"></TextBlock>
        <Grid Grid.Row="21" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SizeMinX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SizeMinY}"/>
        </Grid>

        <TextBlock Grid.Row="22" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Size_max}"></TextBlock>
        <Grid Grid.Row="22" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SizeMaxX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SizeMaxY}"/>
        </Grid>

        <TextBlock Grid.Row="23" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Size_increase}"></TextBlock>
        <Grid Grid.Row="23" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SizeIncreaseX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SizeIncreaseY}"/>
        </Grid>

        <TextBlock Grid.Row="24" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Size_wiggle}"></TextBlock>
        <Grid Grid.Row="24" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SizeWiggleX}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SizeWiggleY}"/>
        </Grid>

        <TextBlock Grid.Row="25" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Speed_min_max}"></TextBlock>
        <Grid Grid.Row="25" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SpeedMin}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SpeedMax}"/>
        </Grid>

        <TextBlock Grid.Row="26" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Speed_increase_wiggle}"></TextBlock>
        <Grid Grid.Row="26" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding SpeedIncrease}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding SpeedWiggle}"/>
        </Grid>

        <TextBlock Grid.Row="27" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Gravity_force_direction}"></TextBlock>
        <Grid Grid.Row="27" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding GravityForce}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding GravityDirection}"/>
        </Grid>

        <TextBlock Grid.Row="28" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Direction_min_max}"></TextBlock>
        <Grid Grid.Row="28" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding DirectionMin}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding DirectionMax}"/>
        </Grid>

        <TextBlock Grid.Row="29" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Direction_increase_wiggle}"></TextBlock>
        <Grid Grid.Row="29" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding DirectionIncrease}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding DirectionWiggle}"/>
        </Grid>

        <TextBlock Grid.Row="30" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Orientation_min_max}"></TextBlock>
        <Grid Grid.Row="30" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding OrientationMin}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding OrientationMax}"/>
        </Grid>

        <TextBlock Grid.Row="31" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Orientation_increase_wiggle}"></TextBlock>
        <Grid Grid.Row="31" Grid.Column="1">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <local:TextBoxDark Grid.Column="0" Margin="3" Text="{Binding OrientationIncrease}"/>
            <local:TextBoxDark Grid.Column="1" Margin="3" Text="{Binding OrientationWiggle}"/>
        </Grid>

        <TextBlock Grid.Row="32" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Is_orientation_relative}"></TextBlock>
        <CheckBox Grid.Row="32" Grid.Column="1" Margin="3" IsChecked="{Binding OrientationRelative}"/>

        <TextBlock Grid.Row="33" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Spawn_on_death}"></TextBlock>
        <local:UndertaleObjectReference Grid.Row="33" Grid.Column="1" Margin="3" ObjectReference="{Binding SpawnOnDeath}" ObjectType="{x:Type undertale:UndertaleParticleSystemEmitter}"/>

        <TextBlock Grid.Row="34" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Spawn_on_death_count}"></TextBlock>
        <local:TextBoxDark Grid.Row="34" Grid.Column="1" Margin="3" Text="{Binding SpawnOnDeathCount}"/>

        <TextBlock Grid.Row="35" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Spawn_on_update}"></TextBlock>
        <local:UndertaleObjectReference Grid.Row="35" Grid.Column="1" Margin="3" ObjectReference="{Binding SpawnOnUpdate}" ObjectType="{x:Type undertale:UndertaleParticleSystemEmitter}"/>

        <TextBlock Grid.Row="36" Grid.Column="0" Margin="3" Text="{x:Static strings:Strings.Spawn_on_update_count}"></TextBlock>
        <local:TextBoxDark Grid.Row="36" Grid.Column="1" Margin="3" Text="{Binding SpawnOnUpdateCount}"/>
    </Grid>
</local:DataUserControl>
